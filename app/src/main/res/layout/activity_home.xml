<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E88B8B"
    android:backgroundTint="#F5F7F8"
    tools:context=".home">

    <Button
        android:id="@+id/btncatr"
        android:layout_width="157dp"
        android:layout_height="87dp"
        android:layout_marginStart="212dp"
        android:layout_marginTop="500dp"
        android:backgroundTint="#DDA552"
        android:text="cataract"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnchat"
        android:layout_width="157dp"
        android:layout_height="87dp"
        android:layout_marginStart="212dp"
        android:layout_marginTop="416dp"
        android:backgroundTint="#3988AC"
        android:text="chat bot"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnchenl"
        android:layout_width="157dp"
        android:layout_height="87dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="416dp"
        android:backgroundTint="#C65D3C"
        android:text="chanel"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnreminder"
        android:layout_width="157dp"
        android:layout_height="87dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="500dp"
        android:backgroundTint="#D64A4A"
        android:text="reminder"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="319dp"
        android:layout_height="51dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/serch_shap"
        android:drawableLeft="@drawable/ic_baseline_saved_search_24"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="Serch"
        android:inputType="textPersonName"
        android:padding="10dp"
        app:circularflow_radiusInDP="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/viewss"
        style="@style/custom_card_view_style_cirle"
        android:layout_width="407dp"
        android:layout_height="135dp"
        android:layout_marginTop="180dp"
        android:background="#C82A2A"
        android:backgroundTint="#314ADA"
        app:cardBackgroundColor="#F6F5F5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="331dp"
            android:layout_height="152dp">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="178dp"
                android:layout_height="29dp"
                android:text="check you eye"
                android:textColor="#EAB107"
                android:textColorHint="#E82020"
                android:textSize="21sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.077"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.137" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="149dp"
                android:layout_height="58dp"
                android:text="check you eye there is catract or not "
                android:textColor="#FFFFFF"
                android:textColorHint="#E82020"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.087"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.542" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="126dp"
                android:layout_height="32dp"
                android:text="scan eye"
                android:textColor="#E89C2B"
                android:textColorHint="#E82020"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.126"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.866" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="128dp"
                android:layout_height="92dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.266"
                app:srcCompat="@drawable/imagedoc" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="185dp"
        android:layout_height="50dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="104dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="35sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="163dp"
        android:layout_height="36dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="364dp"
        android:text="Doctore help"
        android:textAlignment="center"
        android:textColor="#090909"
        android:textSize="@dimen/title"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="116dp"
        android:text="Welcome ! "
        android:textAlignment="center"
        android:textColor="#201A1A"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/view2"
        style="@style/custom_card_vive_home"
        android:layout_width="189dp"
        android:layout_height="83dp"
        android:layout_marginStart="224dp"
        android:layout_marginBottom="4dp"
        android:background="#CD1D1D"
        android:backgroundTint="#E8A249"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>